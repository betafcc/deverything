{
  "name": "semantic-random",
  "version": "0.0.1",
  "description": "Semantic random data generator",
  "main": "dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts",
    "lint": "TIMING=1 eslint src --fix",
    "clean": "rm -rf node_modules && rm -rf dist",
    "prepublish": "pnpm build",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codeledge/semantic-random.git"
  },
  "keywords": [
    "random",
    "generator",
    "fake",
    "testing",
    "data",
    "aleatory",
    "words",
    "numbers",
    "dates"
  ],
  "author": "ogroppo",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@jest/globals": "^29.3.1",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.15",
    "jest": "^29.3.1",
    "ts-node": "^10.9.1",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
  }
}
